{
  "plugins": [
    "html"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "env": {
    "jasmine": true,
    "node": true,
    "mocha": true,
    "browser": true,
    "builtin": true,
    "es6": true,
    "commonjs": true,
    "jquery": true
  },
  "globals": {"ga":false, "FB": false, "Modernizr": false},
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 2,
    "comma-style": [2, "last"],
    "curly": [2, "all"],
    "dot-notation": [2, { "allowKeywords": true }],
    "eqeqeq": [2, "allow-null"],
    "strict": [0, "safe"],
    "guard-for-in": 2,
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-debugger": 2,
    "no-console": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": [2, "functions"],
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-script-url": 1,
    "no-sequences": 2,
    "no-shadow": [2, { "builtinGlobals": false, "hoist": "all" }],
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-with": 2,
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "valid-typeof": 2,
    "wrap-iife": [2, "any"],
    "comma-dangle": 2,
    "no-constant-condition": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-ex-assign": 2,
    "no-extra-semi": 2,
    "no-func-assign": 1,
    "no-inner-declarations": [2, "both"],
    "no-invalid-regexp": 2,
    "no-negated-in-lhs": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-useless-call": 1,
    "radix": 2,
    "vars-on-top": 1,
    "yoda": [2, "never"]
  }
}